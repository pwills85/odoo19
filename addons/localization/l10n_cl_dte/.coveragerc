# Coverage configuration for l10n_cl_dte XXE Security Tests
# Sprint 1.3 - Testing XXE Security

[run]
branch = True
source = libs/
omit =
    */tests/*
    */__pycache__/*
    */migrations/*

[report]
precision = 2
show_missing = True
skip_covered = False

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

[html]
directory = coverage_xxe_html
title = XXE Security Coverage - l10n_cl_dte
